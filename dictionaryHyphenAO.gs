function getHyphenAOMap() {
  const dictio = new Map();

  dictio.set('bem-vindo', ['bem-vinda/o', 'bem-vindo/a']);
  dictio.set('bem-vindos', ['bem-vindas/os', 'bem-vindos/as']);
  dictio.set('não-binário', ['não-binária/o', 'não-binário/a']);
  dictio.set('não-binários', ['não-binárias/os', 'não-binários/as']);

  return dictio;
}
